@article{ ISI:000398770700024,
Author = {Kulczycki, Emanuel and Rozkosz, Ewa A.},
Title = {{Does an expert-based evaluation allow us to go beyond the Impact Factor?
   Experiences from building a ranking of national journals in Poland}},
Journal = {{SCIENTOMETRICS}},
Year = {{2017}},
